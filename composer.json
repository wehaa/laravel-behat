{
  "name": "wehaa/laravel-behat",
  "description": "A Behat implementation for Laravel > 5.5",
  "keywords": [
    "behat",
    "testing",
    "laravel"
  ],
  "homepage": "https://github.com/wehaa/laravel-behat",
  "license": "MIT",
  "authors": [
    {
      "name": "Emiliano Tisato",
      "email": "emil@wehaa.com",
      "homepage": "https://wehaa.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1",
    "illuminate/support": "~5.5.0|~5.6.0|~5.7.0",
  },
  "require-dev": {
    "orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0",
    "phpunit/phpunit" : "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Wehaa\\LaravelBehat\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wehaa\\LaravelBehat\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Wehaa\\LaravelBehat\\LaravelBehatServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}